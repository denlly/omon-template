{
  "name": "omon-template",
  "description": "This a good practise whit nuxt and typescript.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "debug": "cross-env NODE_ENV=development nodemon --inspect server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "tslint -c tslint.json ''client/**/*.ts''",
    "predev": "npm run fonts",
    "fonts": "fontmin client/assets/fonts/**/* client/assets/.fonts/"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.4.1",
    "axios": "^0.18.0",
    "dotenv": "^8.0.0",
    "inversify": "^5.0.1",
    "iview": "^3.4.1",
    "nuxt": "2.6.3",
    "nuxt-i18n": "^5.10.0",
    "nuxt-property-decorator": "2.1.3",
    "reflect-metadata": "^0.1.13",
    "relative": "^3.0.2",
    "ts-node": "8.1.0",
    "tslint-loader": "^3.5.4",
    "vuex-class": "0.3.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "2.6.3",
    "@types/node": "10.14.6",
    "@types/webpack-env": "1.13.9",
    "fontmin": "^0.9.7-beta",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.0.1",
    "tslint": "^5.16.0"
  },
  "engines": {
    "node": "10.x"
  }
}
